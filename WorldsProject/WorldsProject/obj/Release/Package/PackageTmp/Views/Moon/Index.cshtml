@model IEnumerable<StarboundWorlds.Models.Moon>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.MoonName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.WorldType)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @{ item.MoonName = item.inheritMoonName(); }

            @Html.ActionLink(item.MoonName, "Details", new { id = item.MoonID }) 
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.WorldType)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.MoonID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.MoonID })
        </td>
    </tr>
}

</table>
